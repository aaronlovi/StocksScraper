{
    "ConnectionStrings": {
        "stocks-data": "Host=127.0.0.1;Username=postgres;Password=postgres;Database=stocks-data;Include Error Detail=true"
    },
    "DatabaseSchema": "public",
    "Ports": {
        "Grpc": 7001
    },
    "MaxRetries": 5,
    "RetryDelayMilliseconds": 1000,
    "MaxConcurrentStatements": 20,
    "MaxConcurrentReadStatements": 20,
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Elastic.Serilog.Sinks" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console",
                            "Args": {
                                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                            }
                        },
                        {
                            "Name": "Elasticsearch",
                            "Args": {
                                "bootstrapMethod": "Failure",
                                "nodes": [ "http://127.0.0.1:9200" ],
                                "dataStream": "edgar-scraper-debug"
                            }
                        }
                    ]
                }
            }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
    },
    "ElasticConfiguration": {
        "Uri": "http://127.0.0.1:9200"
    },
    "CompanyFactsBulkZipPath": "/var/lib/edgar-data/2025-12-27/companyfacts.zip",
    "EdgarSubmissionsBulkZipPath": "/var/lib/edgar-data/2025-12-27/submissions.zip",
    "EdgarDataDir": "/var/lib/edgar-data/2025-12-27",
    "SecTickerMappings": {
        "UserAgent": "EDGARScraper (contact: inno.and.logic@gmail.com)"
    },
    "StooqPrices": {
        "OutputDir": "/var/lib/edgar-data/2025-12-27/prices/stooq",
        "UserAgent": "EDGARScraper (contact: inno.and.logic@gmail.com)",
        "DelayMilliseconds": 250,
        "MaxRetries": 5
    },
    "StooqImport": {
        "MaxTickersPerRun": 200,
        "BatchSize": 500
    },
    "UsGaap2025ConceptsFileProcessorOptions": {
        "CsvPath": "/var/lib/edgar-data/us-gaap-taxonomies/2025_GAAP_Taxonomy.worksheets.concepts.csv"
    },
    "UsGaap2025PresentationFileProcessorOptions": {
        "CsvPath": "/var/lib/edgar-data/us-gaap-taxonomies/2025_GAAP_Taxonomy.worksheets.presentation.csv"
    }
}
