{
    "ConnectionStrings": {
        "stocks-data": "Host=127.0.0.1;Username=postgres;Password=postgres;Database=stocks-data"
    },
    "DatabaseSchema": "public",
    "Ports": {
        "Grpc": 7001
    },
    "MaxRetries": 5,
    "RetryDelayMilliseconds": 1000,
    "MaxConcurrentStatements": 20,
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "Console",
                            "Args": {
                                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                            }
                        },
                        {
                            "Name": "File",
                            "Args": {
                                "path": "C:\\work\\stocks-data\\logs\\stocks-data-.txt",
                                "rollingInterval": "Day",
                                "rollOnFileSizeLimit": true,
                                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
                            }
                        }
                    ]
                }
            }
        ]
    },
    "CompanyFactsBulkZipPath": "C:\\work\\stocks-data\\2024-12-28\\companyfacts.zip",
    "EdgarSubmissionsBulkZipPath": "C:\\work\\stocks-data\\2024-12-28\\submissions.zip"
}
